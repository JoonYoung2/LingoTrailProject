<h1>Game 1 List</h1>
<table>
    <tr>
        <th>Question</th>
        <th>Answer</th>
        <th>Wrong 1</th>
        <th>Wrong 2</th>
        <th>Wrong 3</th>
        <th>Wrong 3</th>
    </tr>
    
    <% list.forEach( (item) => { %>
        <tr>
            <td><%= item.QUESTION %></td>
            <td><%= item.ANSWER %></td>
            <td><%= item.WRONG1 %></td>
            <td><%= item.WRONG2 %></td>
            <td><%= item.WRONG3 %></td>
        </tr>
    <% }); %>
</table>

<br><br>
<hr>


<a href="/game1/start"><button>랜덤 시작하기</button></a>
<br>
<hr>

<form action="/game1/start" method="post"> 
    <select id="levelSelect" name="level">
      <option value="1">Level 1</option>
      <option value="2">Level 2</option>
      <option value="3">Level 3</option>
      <option value="4">Level 4</option>
      <option value="5">Level 5</option>
    </select>
  <button type="submit" id="startButton">시작하기</button>
</form>